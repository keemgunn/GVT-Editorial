{
  "name": "gvt-editorial",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "DEV_0": "export NODE_ENV=DEV_0 && cd template && npm run sass-glob-restore && cd .. && vite",
    "DEV_1": "export NODE_ENV=DEV_1 && cd template && npm run sass-glob-restore && cd .. && vite",
    "DEV_2": "export NODE_ENV=DEV_2 && cd template && npm run sass-glob-restore && cd .. && vite",
    "BUILD & PREV": "npm run build && npm run preview",

    "build": "export NODE_ENV=BUILD && cd template && npm run sass-glob-builder && cd .. && vite build",
    "preview": "vite preview"
  },
