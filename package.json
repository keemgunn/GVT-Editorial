{
  "name": "gvt-editorial",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev-0": "export NODE_ENV=DEV_0 && npm run sass-glob-restore && vite",
    "dev-1": "export NODE_ENV=DEV_1 && npm run sass-glob-restore && vite",
    "dev-2": "export NODE_ENV=DEV_2 && npm run sass-glob-restore && vite",
    "build-and-preview": "npm run build && npm run preview",
    "build": "export NODE_ENV=BUILD && npm run sass-glob-builder && vite build",
    "preview": "vite preview",
    "build-and-typecheck": "run-p type-check build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "test:unit": "vitest --environment jsdom --root src/",
    "sass-glob-builder": "source ./src/template/tools/pyenv_3_11/bin/activate && python ./src/template/tools/sass-glob-builder.py",
    "sass-glob-restore": "source ./src/template/tools/pyenv_3_11/bin/activate && python ./src/template/tools/sass-glob-restore.py"
  },
  "dependencies": {
    "@js-temporal/polyfill": "^0.4.3",
    "@vueuse/head": "^1.0.25",
    "cross-env": "^7.0.3",
    "http-status-codes": "^2.2.0",
    "moment": "^2.29.4",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.0.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "jsdom": "^20.0.3",
    "markdown-it-anchor": "^8.6.6",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-prism": "^2.3.0",
    "markdown-it-toc-done-right": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prismjs": "^1.29.0",
    "sass": "^1.57.1",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vite-plugin-sass-glob-import": "^2.0.0",
    "vite-plugin-vue-markdown": "^0.22.4",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12"
  }
}
